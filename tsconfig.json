{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "moduleResolution": "node",
    "importsNotUsedAsValues": "remove"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
