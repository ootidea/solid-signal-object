{
  "name": "solid-signal-object",
  "version": "1.1.0",
  "description": "A thin wrapper library for SolidJS signals, which can solve a issue related to narrowing.",
  "keywords": [
    "solidjs",
    "solid-js",
    "signals",
    "state-management"
  ],
  "homepage": "https://github.com/ootidea/solid-signal-object#readme",
  "bugs": {
    "url": "https://github.com/ootidea/solid-signal-object/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ootidea/solid-signal-object.git"
  },
  "license": "CC0-1.0",
  "author": "ootidea",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "fix": "run-p \"fix:*\"",
    "fix:biome": "biome check --write",
    "fix:package-json": "sort-package-json",
    "check": "run-p \"check:*\"",
    "check:type": "tsc --noEmit",
    "check:biome": "biome check",
    "postversion": "git push && git push --tags && npm publish ./",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@tsconfig/strictest": "^2.0.5",
    "npm-run-all2": "^6.2.3",
    "solid-js": "^1.8.22",
    "sort-package-json": "^2.10.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-plugin-dts": "^4.2.1"
  },
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "packageManager": "pnpm@9.11.0"
}
